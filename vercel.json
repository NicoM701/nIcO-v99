{
  "framework": null,
  "buildCommand": "",
  "outputDirectory": ".",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/settings", "destination": "/settings.html" },
    { "source": "/faq", "destination": "/faq.html" }
  ],
  "headers": [
    {
      "source": "/(icons|assets)/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
